{
  "name": "voxalgo",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "scripts": {
    "start:api": "nest start api --watch",
    "start:example": "nest start example --watch",
    "start:voice_gateway": "nest start voice_gateway --watch",
    "prebuild": "rimraf dist",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "docker:start": "docker-compose up -d --build",
    "start:dev": "nest start --watch",
    "db:setup": "npm run docker:start && npm run migrate:dev && npm run generate",
    "dev": "npx concurrently \"npm run start:dev api\" \"npm run start:dev example\" \"npm run start:dev voice-gateway\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/api/test/jest-e2e.json",
    "build:api": "nest build api",
    "build:example": "nest build example",
    "migrate:dev": "npx prisma migrate dev --schema=./libs/prisma/prisma/schema.prisma",
    "generate": "npx prisma generate --schema=./libs/prisma/prisma/schema.prisma"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^9.2.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "9.0.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/websockets": "9.0.0",
    "@prisma/client": "^5.22.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "nats": "^2.28.2",
    "openai": "^4.72.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^9.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "prisma": "^5.22.0",
    "rimraf": "^4.0.0",
    "source-map-support": "^0.5.20",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "overrides": {
    "rimraf": "^4.0.0",
    "glob": "^9.0.0"
  }
}
